            <string>var resultid = gj_result_id()

//If it was user information.
if resultid = request[0]
{
    var status = gj_result_status();
    if status = 1
    {
        gj_session_open();
        alarm[1] = 1
        request[2] = gj_trophy_get_id("5035");
        gj_trophy_add("5035")
    }
    else
    if status = 0
    {
        obj_error_box.textbox = (string(gj_result_error()));
        obj_error_box.error = true
    }
    else
    {
        obj_error_box.textbox = ("GameMaker Error");
        obj_error_box.error = true
    }
}
else
//If it was for fetching trophies
if resultid = request[2]
{
    var status = gj_result_status();
    if status = 1
    {
        total = gj_result_data_count(); // Total trophies loaded
        for ( i = 0; i &lt; total; i++)
        {
            title = gj_result_data_field_num( "title", i); // Fetch title of trophy
            achieved = gj_result_data_field_num( "achieved", i); // Fetch if trophy is achieved
            if achieved = "false" or achieved = "just now"
            {
                ds_queue_enqueue(obj_gj_achievements.title,title)
            }
            //"achieved", "title" are available in the manual
        }
    }
    else
    {
        obj_error_box.textbox = (string(gj_result_error()));
        obj_error_box.error = true
    }
}


gj_result_clear()
</string>
